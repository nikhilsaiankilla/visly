{
    "name": "visly-sdk",
    "version": "0.2.3",
    "type": "module",
    "scripts": {
        "clean": "node -e \"try{require('fs').rmSync('dist',{recursive:true,force:true})}catch{}\"",
        "build": "npm run clean && npm run build:index && npm run build:react && npm run build:vue && npm run build:angular && npm run build:types",
        "build:index": "tsup src/index.ts --format esm,cjs --dts --sourcemap --target es2022 --clean",
        "build:react": "tsup src/react.tsx --format esm,cjs --dts --sourcemap --target es2022",
        "build:vue": "tsup src/vue.ts --format esm,cjs --dts --sourcemap --target es2022",
        "build:angular": "tsup src/angular.ts --format esm,cjs --dts --sourcemap --target es2022",
        "build:types": "tsc -p tsconfig.json --emitDeclarationOnly false"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./react": {
            "import": "./dist/react.js",
            "require": "./dist/react.cjs"
        },
        "./vue": {
            "import": "./dist/vue.js",
            "require": "./dist/vue.cjs"
        },
        "./angular": {
            "import": "./dist/angular.js",
            "require": "./dist/angular.cjs"
        }
    },
    "typesVersions": {
        "*": {
            "react": [
                "dist/react.d.ts"
            ],
            "vue": [
                "dist/vue.d.ts"
            ],
            "angular": [
                "dist/angular.d.ts"
            ]
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "peerDependencies": {
        "@angular/core": ">=15",
        "@angular/router": ">=15",
        "react": ">=18",
        "rxjs": ">=7",
        "vue": ">=3"
    },
    "devDependencies": {
        "@types/react": "^19.2.7",
        "tsup": "^8.0.1",
        "typescript": "^5.6.3"
    },
    "publishConfig": {
        "access": "public"
    },
    "license": "MIT",
    "description": "",
    "keywords": [],
    "author": ""
}
